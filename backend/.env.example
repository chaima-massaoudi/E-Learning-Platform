# Environment Variables Example
# Copy this file to .env and fill in your values

# MongoDB Connection
MONGO_URI=mongodb+srv://your_username:your_password@cluster0.xxxxx.mongodb.net/elearning?retryWrites=true&w=majority

# JWT Secret (generate a random string)
JWT_SECRET=your_super_secret_jwt_key_here_change_this

# JWT Expiration
JWT_EXPIRE=30d

# Server Port
PORT=5000

# Node Environment
NODE_ENV=development
